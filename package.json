    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "tauri": "tauri",
    "lint": "prettier --check src/ && eslint src/",
    "lint:fix": "eslint src/ --fix && prettier --write src/",
